{
  "name": "terratruce-mcp-backend",
  "version": "1.0.0",
  "description": "MCP-based geospatial intelligence backend for Tera Truce",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "verify": "tsc && node dist/verify.js",
    "dev": "tsc && NODE_ENV=development node dist/index.js",
    "watch": "tsc --watch",
    "api": "tsc && node dist/api-server.js",
    "dev:api": "tsc && NODE_ENV=development node dist/api-server.js"
  },
  "keywords": [
    "mcp",
    "geospatial",
    "real-estate",
    "ai"
  ],
  "author": "Tera Truce Team",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@supabase/supabase-js": "^2.88.0",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "googleapis": "^169.0.0",
    "ioredis": "^5.4.1",
    "nodemailer": "^7.0.12",
    "resend": "^6.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.10.5",
    "@types/nodemailer": "^7.0.4",
    "typescript": "^5.7.2"
  }
}
